<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <TitledPane animated="false" layoutX="0.0" layoutY="-30.0" prefHeight="530.0" prefWidth="500.0" text="Rowdy Run 2.0">
      <content>
        <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
      </content>
    </TitledPane>
    <AnchorPane prefHeight="500.0" prefWidth="500.0">
      <children>
        <Canvas fx:id="canvas" height="500.0" visible="true" width="500.0" />
        <GridPane alignment="CENTER" layoutX="185.0" layoutY="145.0" prefWidth="130.0" vgap="40.0" visible="false">
          <children>
            <Button mnemonicParsing="false" prefWidth="130.0" text="Continue" GridPane.rowIndex="1">
              <font>
                <Font size="24.0" fx:id="x1" />
              </font>
            </Button>
            <Button font="$x1" mnemonicParsing="false" prefWidth="130.0" text="Quit" GridPane.rowIndex="2" />
            <Label alignment="CENTER" prefHeight="56.0" prefWidth="130.0" text="Pause" textAlignment="CENTER">
              <font>
                <Font size="36.0" />
              </font>
            </Label>
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <padding>
            <Insets bottom="20.0" top="20.0" />
          </padding>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
        </GridPane>
        <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@../../images/bg.png" />
          </image>
        </ImageView>
      </children>
    </AnchorPane>
  </children>
</AnchorPane>
